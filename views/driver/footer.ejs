  function changeActiveMenu(){
    let url=location.href;
    
    if(url.includes("/task")||url.includes("driver_list")||url.includes("edit_driver_details")){
    document.getElementById("dash").classList.remove("active");
    document.getElementById("task").classList.add("active");
    }

    if(url.includes("/completed-task")||url.includes("/order-list/")||url.includes("/order-inv/")||url.includes("/expense-list/")){
    document.getElementById("dash").classList.remove("active");
    document.getElementById("c-task").classList.add("active");
    }
    if(url.includes("/salary")){
    document.getElementById("dash").classList.remove("active");
    document.getElementById("salary").classList.add("active");
    }
    if(url.includes("/leave")){
    document.getElementById("dash").classList.remove("active");
    document.getElementById("leave").classList.add("active");
    }
  }

  changeActiveMenu();
